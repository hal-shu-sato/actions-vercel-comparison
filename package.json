{
  "name": "next-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "run-s fix:next fix:prettier",
    "fix:next": "next lint --fix",
    "fix:prettier": "prettier --write .",
    "lint": "run-s -c lint:ts lint:next lint:prettier",
    "lint:next": "next lint",
    "lint:prettier": "prettier --check .",
    "lint:ts": "tsc --incremental false --noEmit",
    "prepare": "husky",
    "start": "next start"
  },
  "dependencies": {
    "next": "^14.2.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.35",
    "eslint-config-prettier": "^9.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@1.22.22"
